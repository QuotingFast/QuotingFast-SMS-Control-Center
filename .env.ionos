# QuotingFast SMS Control Center - IONOS Production Environment
# Replace placeholders with your actual values

# Application
NODE_ENV=production
PORT=8080  # Or the port IONOS assigns to Node.js applications

# Database
DATABASE_URL=postgresql://username:password@your-db-host:5432/your-db-name
# JWT
JWT_SECRET=your_secure_jwt_secret
JWT_EXPIRE=30d

# TCPA Hours
TCPA_START_HOUR=8
TCPA_END_HOUR=21

# OpenAI
OPENAI_API_KEY=your_openai_api_key_here

# Elevenlabs
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Twilio
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_PHONE_NUMBER=your_twilio_phone_number_here
# Application URLs
APP_URL=https://quotingfast.io
REDIRECT_URL=https://quotingfast.io/quote
